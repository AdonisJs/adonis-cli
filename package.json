{
  "name": "@adonisjs/cli",
  "version": "4.0.12",
  "description": "Command line tool for Adonisjs",
  "main": "build/index.js",
  "bin": {
    "adonis": "build/index.js"
  },
  "files": [
    "build/src",
    "build/ace-manifest.json",
    "build/index.d.ts",
    "build/index.js"
  ],
  "keywords": [
    "adonis",
    "adonisjs",
    "cli",
    "installer"
  ],
  "author": "Harminder Virk <virk@adonisjs.com>",
  "contributors": [
    "Romain Lanz <romain.lanz@slynova.ch>"
  ],
  "license": "MIT",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "prepublishOnly": "npm run build",
    "prepare": "npm run prepublishOnly",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && node build/generateManifest.js",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-cli.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-cli/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-cli#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.0.3",
    "@types/node": "^12.6.8",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.0.1",
    "japa": "^2.0.10",
    "mrm": "^1.2.2",
    "pkg-ok": "^2.3.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/ace": "^6.1.2",
    "@adonisjs/fold": "^6.1.1",
    "@adonisjs/sink": "^2.0.7",
    "@poppinss/application": "^1.0.7",
    "@poppinss/chokidar-ts": "^1.0.4",
    "boxen": "^4.1.0",
    "copyfiles": "^2.1.1",
    "del": "^5.0.0",
    "execa": "^2.0.3",
    "fs-extra": "^8.1.0",
    "kleur": "^3.0.3",
    "listr": "^0.14.3",
    "listr-update-renderer": "^0.5.0",
    "nanomatch": "^1.2.13",
    "rxjs": "^6.5.2",
    "semver": "^6.3.0"
  }
}
